        <xsd:annotation>
          <xsd:documentation>ссудный счет на котором учитывается движение задолженности по предоставленным кредитам</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="08">
        <xsd:annotation>
          <xsd:documentation>карточный счет открываемый для карточных проектов</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="09">
        <xsd:annotation>
          <xsd:documentation>бюджетный счет открываемый лицам, осуществляющим операции со средствами бюджетов всех уровней и государственных внебюджетных фондов</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="10">
        <xsd:annotation>
          <xsd:documentation>корреспондентский счет открываемый кредитным организация</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="11">
        <xsd:annotation>
          <xsd:documentation>корреспондентский субсчет открываемый филиалам кредитных организаций</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="12">
        <xsd:annotation>
          <xsd:documentation>счет доверительного управления открываемый доверительному управляющему</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="13">
        <xsd:annotation>
          <xsd:documentation>специальный банковский счет открываемый юрлицам, физлицам в рамках операций с ценными бумагами</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="14">
        <xsd:annotation>
          <xsd:documentation>совместный счет открываемый несколькими лицам</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="15">
        <xsd:annotation>
          <xsd:documentation>номинальный счет открываемый лицу, осуществляющему операции со средствами, принадлежащими другому лицу</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="16">
        <xsd:annotation>
          <xsd:documentation>накопительный счет создаваемого юрлица</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
