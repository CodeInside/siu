  <!--

  Ведомственные классификаторы

  -->

  <xsd:simpleType name="PersonInnType">
    <xsd:annotation>
      <xsd:documentation>ИНН Физического лица</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:length value="12" />
      <xsd:pattern value="([0-9]{1}[1-9]{1}|[1-9]{1}[0-9]{1})[0-9]{10}" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="CompanyInnType">
    <xsd:annotation>
      <xsd:documentation>ИНН юридического лица</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:length value="10" />
      <xsd:pattern value="([0-9]{1}[1-9]{1}|[1-9]{1}[0-9]{1})[0-9]{8}" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="InnType">
    <xsd:annotation>
      <xsd:documentation>ИНН</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="10" />
      <xsd:maxLength value="12" />
      <xsd:pattern value="(([0-9]{1}[1-9]{1}|[1-9]{1}[0-9]{1})[0-9]{8})|(([0-9]{1}[1-9]{1}|[1-9]{1}[0-9]{1})[0-9]{10})" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="KppType">
    <xsd:annotation>
      <xsd:documentation>КПП</xsd:documentation>
