  --><xsd:schema xmlns:tns="http://www.red-soft.biz/schemas/fssp/common/2011/0.5" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xmime="http://www.w3.org/2005/05/xmlmime" targetNamespace="http://www.red-soft.biz/schemas/fssp/common/2011/0.5" elementFormDefault="unqualified" attributeFormDefault="unqualified">

  <!--

  Базовые типы данных

  -->

  <xsd:simpleType name="Integer">
    <xsd:annotation>
      <xsd:documentation>Целое</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:integer" />
  </xsd:simpleType>

  <xsd:simpleType name="PositiveShort">
    <xsd:annotation>
      <xsd:documentation>Положительное короткое целое</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:unsignedShort">
      <xsd:minInclusive value="1" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="InternalKeyType">
    <xsd:annotation>
      <xsd:documentation>Ключ объекта, присвоенний информационной системой ФССП РФ при создании объекта или при
        получении объекта от контрагента
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:integer">
      <xsd:minInclusive value="-9223372036854775808" />
      <xsd:maxInclusive value="9223372036854775807" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="ExternalKeyType">
    <xsd:annotation>
      <xsd:documentation>Идентификатор объекта, присвоенный объекту АИС внешнего
        контрагентом (ФНС, ПФР и прочие). Данный идентификатор должен быть уникальным для типа
        идентифицируемого документа в пространстве обмена данными с конкретным внешним
        контрагентом.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:minLength value="1" />
      <xsd:maxLength value="60" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="Money">
    <xsd:annotation>
      <xsd:documentation>Сумма</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:decimal">
      <xsd:totalDigits value="15" />
      <xsd:fractionDigits value="2" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="Exchange">
    <xsd:annotation>
      <xsd:documentation>Курс валюты</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:decimal">
      <xsd:totalDigits value="15" />
      <xsd:fractionDigits value="4" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="Quantity">
    <xsd:annotation>
      <xsd:documentation>Количество</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:decimal">
      <xsd:totalDigits value="15" />
      <xsd:fractionDigits value="3" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="Area">
    <xsd:annotation>
      <xsd:documentation>Площадь</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:decimal">
      <xsd:totalDigits value="20" />
      <xsd:fractionDigits value="2" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="Date">
    <xsd:annotation>
      <xsd:documentation>Дата</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:date">
      <xsd:minInclusive value="1900-01-01" />
      <xsd:maxExclusive value="2100-01-01" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="DateTime">
    <xsd:annotation>
      <xsd:documentation>Дата и время</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:dateTime">
      <xsd:minInclusive value="1900-01-01T00:00:00" />
      <xsd:maxExclusive value="2100-01-01T00:00:00" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="Boolean">
    <xsd:annotation>
      <xsd:documentation>Логический</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:boolean" />
  </xsd:simpleType>

  <xsd:simpleType name="Blob">
    <xsd:annotation>
      <xsd:documentation>Массив произвольных данных (бинарный)</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string" />
  </xsd:simpleType>

