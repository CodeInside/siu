  --><xsd:schema xmlns:tns="http://www.red-soft.biz/schemas/fssp/common/2011/0.5" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.red-soft.biz/schemas/fssp/common/2011/0.5" elementFormDefault="unqualified" attributeFormDefault="unqualified">

    <xsd:include schemaLocation="ECE4B24E-2850-4DD3-9C73-DEF9EA926712.XSD" />


    <xsd:complexType name="LookupTableQuery">
        <xsd:annotation>
            <xsd:documentation>Запрос данных справочника</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="lookupTable" type="tns:LookupTableType">
                <xsd:annotation>
                    <xsd:documentation>Тип справочника</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="filter" type="tns:String_1_1000" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Дополнительное условие</xsd:documentation>
                    <xsd:documentation>Указание данного значения позволяет при необходимости получить лишь часть справочника.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="LookupTableData">
        <xsd:annotation>
            <xsd:documentation>Данные справочника</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="lookupTable" type="tns:LookupTableType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Тип справочника</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="filter" type="tns:String_1_1000" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Дополнительное условие</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="osp" type="tns:OspProperty">
                    <xsd:annotation>
                        <xsd:documentation>Отдел судебных приставов</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="idDocType" type="tns:IdDocTypeProperty">
                    <xsd:annotation>
                        <xsd:documentation>Тип документа, удостоверяющего личность</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="currency" type="tns:CurrencyProperty">
                    <xsd:annotation>
                        <xsd:documentation>Валюта</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="pievFormat" type="tns:PievFormatVersion">
                    <xsd:annotation>
                        <xsd:documentation>Версии форматов ПИЭВ</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="OspProperty" type="tns:OspProperty" />
    <xsd:complexType name="OspProperty">
        <xsd:annotation>
            <xsd:documentation>Контейнер OspProperty «Отдел СП»</xsd:documentation>
            <xsd:documentation>Содержит сведения о расчетном счете отдела СП</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="InternalKey" type="tns:InternalKeyType">
                <xsd:annotation>
                    <xsd:documentation>Ключ ФССП России</xsd:documentation>
                    <xsd:documentation>Уникальный идентификатор объекта, присвоенный АИС ФССП России</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="OspCode" type="tns:VkspType">
                <xsd:annotation>
                    <xsd:documentation>Код отдела СП</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="BeginDate" type="tns:Date" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Дата начала действия</xsd:documentation>
                    <xsd:documentation>Дата начала действия реквизитов, Неуказанное значение сигнализирует о том, что реквизиты действуют на момент ответа на запрос</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="EndDate" type="tns:Date" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Дата окончания действия</xsd:documentation>
                    <xsd:documentation>Дата окончания действия реквизитов. Неуказанное значение сигнализирует о том, что срок окончения действия реквизитов неизвестен</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="RecpName" type="tns:String_1_300">
                <xsd:annotation>
                    <xsd:documentation>Наименование ОСП для платежей</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="RecpAdr" type="tns:String_1_200">
                <xsd:annotation>
                    <xsd:documentation>Адрес получателя</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="RecpBank" type="tns:String_1_80">
                <xsd:annotation>
                    <xsd:documentation>Банк получателя.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="RecpBIK" type="tns:BikType">
                <xsd:annotation>
                    <xsd:documentation>БИК банка получателя.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="RecpCnt" type="tns:String_1_20">
                <xsd:annotation>
                    <xsd:documentation>Расчетный счет.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="RecpINN" type="tns:CompanyInnType">
                <xsd:annotation>
                    <xsd:documentation>ИНН получателя.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="RecpKPP" type="tns:KppType">
                <xsd:annotation>
                    <xsd:documentation>КПП получателя.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="OKATO" type="tns:OkatoType">
                <xsd:annotation>
                    <xsd:documentation>Код ОКАТО</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="OKTMO" type="tns:OktmoType" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>ОКТМО</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="Address" type="tns:String_1_300" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Адрес подразделения</xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="Details" type="tns:OspDetail" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Сведения об отделе</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="OspDetail">
        <xsd:annotation>
            <xsd:documentation>Контейнер OspDetail «Сведения об отделе судебных приставов»</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="Priority" type="tns:PositiveShort">
                <xsd:annotation>
                    <xsd:documentation>Приоритет</xsd:documentation>
                    <xsd:documentation>Допустимы значения 1 — 10</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="OKATO" type="tns:OkatoType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>ОКАТО</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="OKTMO" type="tns:OktmoType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>ОКТМО</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MinAmount" type="tns:Money" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Сумма от</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MaxAmount" type="tns:Money" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Сумма до</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="IDType" type="tns:IdType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Тип ИД</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ClaimerType" type="tns:ContragentType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Тип органа, выдавшего ИД</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="IdSubj" type="tns:IdSubjType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Тип предмета исполнения</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DebtorType" type="tns:ContragentType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Тип должника</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="KladrStreet" type="tns:KladrCodeL2" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Код КЛАДР улицы</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Street" type="tns:String_1_60" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Название улицы</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="HouseNumber" type="tns:String_1_100" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Дома</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="EvenHouse" type="tns:Boolean" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Все четные дома</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="UnevenHouse" type="tns:Boolean" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Все нечетные дома</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="CurrencyProperty">
        <xsd:annotation>
            <xsd:documentation>Валюты</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="codeNum" type="tns:CurrencyType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Код валюты цифровой</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="codeAlpha" type="tns:String_1_3" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Код валюты буквенный</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="name" type="tns:String_1_50" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Наименование валюты</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="IdDocTypeProperty">
        <xsd:annotation>
            <xsd:documentation>Код типа документа, удостоверяющего личность</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="code" type="tns:IdentificationType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Код вида документа</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="name" type="tns:String_1_150" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Наименование вида документа</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="description" type="tns:String_1_1000" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Описание документа</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
	
	<xsd:complexType name="PievFormatVersion">
        <xsd:annotation>
            <xsd:documentation>Версии форматов ПИЭВ</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="formatName" type="tns:MetaobjectName" minOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>Формат представления</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="namespaceName" type="tns:String_1_1000" minOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>Пространство имен</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="xsdSchema" type="tns:Blob" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>XSD-схема</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
			<xsd:element name="xsltTemplate" type="tns:Blob" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Шаблон xslt-преобразования</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
			<xsd:element name="endDate" type="tns:Date" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Дата окончания</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
	
</xsd:schema>