                <xs:documentation>1 – индивидуальный предприниматель;
2 – глава крестьянского фермерского хозяйства.</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:length value="1"/>
                  <xs:enumeration value="1"/>
                  <xs:enumeration value="2"/>
