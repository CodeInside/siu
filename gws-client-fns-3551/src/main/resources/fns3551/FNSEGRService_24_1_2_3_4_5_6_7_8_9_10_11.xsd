<?xml version="1.0" encoding="UTF-8"?>
<!--Published by JAX-WS RI at http://jax-ws.dev.java.net. RI's version is Metro/2.1.1 (branches/2.1-6844; 2011-07-29T12:07:24+0000) JAXWS-RI/2.2.5 JAXWS/2.2.--><!--edited with XMLSpy v2011 rel. 2 (http://www.altova.com) by TeaM DJiNN (TeaM DJiNN)--><!--edited with XMLSPY v2004 rel. 4 U (http://www.xmlspy.com) by Home (Home)--><xs:schema targetNamespace="http://ws.unisoft/EGRNXX/ResponseVIPFL" elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:doc="urn:exch:document:1.0" xmlns:ds="http://www.w3.org/2000/09/xmldsig#" xmlns:u="http://www.w3.org/2001/XMLSchema1" xmlns:i="http://www.w3.org/2001/XMLSchema2" xmlns:tns="http://ws.unisoft/EGRNXX/ResponseVIPFL">
  <xs:element name="Документ">
    <xs:annotation>
      <xs:documentation>Выписка из ЕГРИП в отношении индивидуального предпринимателя</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice>
        <xs:element name="СвИП">
          <xs:annotation>
            <xs:documentation>Сведения об индивидуальном предпринимателе</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="СвФЛ">
                <xs:annotation>
                  <xs:documentation>Сведения, идентифицирующие  физическое лицо - индивидуального предпринимателя (главу крестьянского (фермерского) хозяйства)</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="ФИОРус" type="tns:ФИОТип">
                      <xs:annotation>
                        <xs:documentation>Фамилия, имя, отчество физического лица на русском языке</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="ФИОЛат" type="tns:ФИОТип" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Фамилия, имя, отчество физического лица с использованием букв латинского алфавита</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="СвРожд" type="tns:СведРождТип" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Сведения о рождении</xs:documentation>
                        <xs:documentation>Ограниченный доступ. Выгружаются только органам, которым по действующему законодательству предоставляются указанные сведения</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="ДатаНачДейств" use="required">
                    <xs:annotation>
                      <xs:documentation>Дата внесения сведений в ЕГРИП</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:date"/>
                    </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="ГРНИП" type="tns:ОГРНИПТип" use="optional">
                    <xs:annotation>
                      <xs:documentation>Государственный регистрационный номер записи, на основании которой сведения внесены в ЕГРИП</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="Пол" use="required">
                    <xs:annotation>
                      <xs:documentation>Пол</xs:documentation>
                      <xs:documentation>1 - мужской;
2 - женский</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:length value="1"/>
                        <xs:enumeration value="1"/>
                        <xs:enumeration value="2"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
              <xs:element name="СвГражд">
                <xs:annotation>
                  <xs:documentation>Сведения о гражданстве</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:attribute name="ДатаНачДейств" use="required">
                    <xs:annotation>
                      <xs:documentation>Дата внесения сведений в ЕГРИП</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:date"/>
                    </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="ГРНИП" type="tns:ОГРНИПТип" use="optional">
                    <xs:annotation>
                      <xs:documentation>Государственный регистрационный номер записи, на основании которой сведения внесены в ЕГРИП</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ВидГражд" use="required">
                    <xs:annotation>
                      <xs:documentation>Вид гражданства</xs:documentation>
                      <xs:documentation>1 - для гражданина Российской Федерации;
2 - для иностранного гражданина;
3 - для лица без гражданства</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:length value="1"/>
                        <xs:enumeration value="1"/>
                        <xs:enumeration value="2"/>
                        <xs:enumeration value="3"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="ОКСМ" type="tns:ОКСМТип" use="optional">
                    <xs:annotation>
                      <xs:documentation>Код страны, гражданином которой является физическое лицо</xs:documentation>
                      <xs:documentation>Реквизит формируется только в отношении иностранного гражданина. При этом указывается 3-х значный цифровой код по общероссийскому классификатору стран мира (ОКСМ)</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="НаимСтран" use="optional">
                    <xs:annotation>
                      <xs:documentation>Наименование страны по справочнику ОКСМ, гражданином которой является физическое лицо</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="250"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
              <xs:element name="СвАдрМЖ" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Сведения об адресе (месте жительства) индивидуального предпринимателя</xs:documentation>
                  <xs:documentation>Ограниченный доступ. Выгружаются только органам, которым по действующему законодательству предоставляются указанные сведения</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="АдресРФ" type="tns:АдрРФТипВып">
                      <xs:annotation>
                        <xs:documentation>Адрес (место жительства) индивидуального предпринимателя</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="ДатаНачДейств" use="required">
                    <xs:annotation>
                      <xs:documentation>Дата внесения сведений в ЕГРИП</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:date"/>
                    </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="ГРНИП" type="tns:ОГРНИПТип" use="optional">
                    <xs:annotation>
                      <xs:documentation>Государственный регистрационный номер записи, на основании которой сведения внесены в ЕГРИП</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
              <xs:element name="СвУдЛичнФЛ" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Сведения о документе, удостоверяющем личность физического лица</xs:documentation>
                  <xs:documentation>Ограниченный доступ. Выгружаются только органам, которым по действующему законодательству предоставляются указанные сведения</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="УдЛичнФЛ" type="tns:УдЛичнТипР">
                      <xs:annotation>
                        <xs:documentation>Данные документа, удостоверяющего личность</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="ДатаНачДейств" use="required">
                    <xs:annotation>
                      <xs:documentation>Дата внесения сведений в ЕГРИП</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:date"/>
                    </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="ГРНИП" type="tns:ОГРНИПТип" use="optional">
                    <xs:annotation>
                      <xs:documentation>Государственный регистрационный номер записи, на основании которой сведения внесены в ЕГРИП</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
              <xs:element name="СвПравЖитРФ" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Сведения о документе, подтверждающем право иностранного гражданина или лица без гражданства временно или постоянно проживать на территории Российской Федерации</xs:documentation>
                  <xs:documentation>Ограниченный доступ. Выгружаются только органам, которым по действующему законодательству предоставляются указанные сведения</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="ДокПравЖитРФ" type="tns:УдЛичнТипР">
                      <xs:annotation>
                        <xs:documentation>Вид и данные документа, подтверждающего право физического лица временно или постоянно проживать на территории Российской Федерации</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="ДатаНачДейств" use="required">
                    <xs:annotation>
                      <xs:documentation>Дата внесения сведений в ЕГРИП</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:date"/>
                    </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="ГРНИП" type="tns:ОГРНИПТип" use="optional">
                    <xs:annotation>
                      <xs:documentation>Государственный регистрационный номер записи, на основании которой сведения внесены в ЕГРИП</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="СрокДействДок" type="xs:date" use="required">
                    <xs:annotation>
                      <xs:documentation>Срок действия документа</xs:documentation>
                      <xs:documentation>Указывается дата до которой действует документ, подтверждающий право на проживание на территории Российской Федерации иностранного гражданина или лица без гражданства</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
              <xs:element name="СвОКВЭД" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>Сведения о кодах по Общероссийскому классификатору видов экономической деятельности</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:attribute name="ДатаНачДейств" use="required">
                    <xs:annotation>
                      <xs:documentation>Дата внесения сведений в ЕГРИП</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:date"/>
                    </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="ГРНИП" type="tns:ОГРНИПТип" use="optional">
                    <xs:annotation>
                      <xs:documentation>Государственный регистрационный номер записи, на основании которой сведения внесены в ЕГРИП</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ПрОснДоп" use="required">
                    <xs:annotation>
                      <xs:documentation>Признак: основной или дополнительный вид деятельности</xs:documentation>
                      <xs:documentation>1 - при передаче кода основного вида деятельности;
2 - при передаче кода дополнительного вида деятельности</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:length value="1"/>
                        <xs:enumeration value="1"/>
                        <xs:enumeration value="2"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="КодОКВЭД" type="tns:ОКВЭДТип" use="required">
                    <xs:annotation>
                      <xs:documentation>Код по Общероссийскому классификатору видов экономической деятельности</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
              <xs:element name="СвСтатус_РО">
                <xs:annotation>
                  <xs:documentation>Сведения о правоспособности (статусе) индивидуального предпринимателя и регистрирующем органе по месту жительства индивидуального предпринимателя</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="СвСтатус">
                      <xs:annotation>
                        <xs:documentation>Сведения о правоспособности (статусе) индивидуального предпринимателя</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:attribute name="КодСтатус" use="required">
                          <xs:annotation>
                            <xs:documentation>Код статуса индивидуального предпринимателя по справочнику СИПСТ</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:length value="3"/>
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="НаимСтатус" use="required">
                          <xs:annotation>
                            <xs:documentation>Наименование статуса индивидуального предпринимателя по справочнику СИПСТ</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:minLength value="5"/>
                              <xs:maxLength value="500"/>
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="ДатаПрекрИП" use="optional">
                          <xs:annotation>
                            <xs:documentation>Дата прекращения деятельности в качестве индивидуального предпринимателя</xs:documentation>
                            <xs:documentation>Формируется в случае, если передается статус, соответствующий прекращению деятельности в качестве индивидуального предпринимателя</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:date"/>
                          </xs:simpleType>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="СвРегОрг" type="tns:СвРегОргТип">
                      <xs:annotation>
                        <xs:documentation>Сведения о регистрирующем (налоговом) органе по месту жительства индивидуального предпринимателя</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="ДатаНачДейств" use="required">
                    <xs:annotation>
                      <xs:documentation>Дата внесения сведений в ЕГРИП</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:date"/>
                    </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="ГРНИП" type="tns:ОГРНИПТип" use="optional">
                    <xs:annotation>
                      <xs:documentation>Государственный регистрационный номер записи, на основании которой сведения внесены в ЕГРИП</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
              <xs:element name="СвРегИП">
                <xs:annotation>
                  <xs:documentation>Сведения о регистрации в качестве индивидуального предпринимателя</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:attribute name="ДатаНачДейств" use="required">
                    <xs:annotation>
                      <xs:documentation>Дата внесения сведений в ЕГРИП</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:date"/>
                    </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="ГРНИП" type="tns:ОГРНИПТип" use="optional">
                    <xs:annotation>
                      <xs:documentation>Государственный регистрационный номер записи, на основании которой сведения внесены в ЕГРИП</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ДатаРег" use="required">
                    <xs:annotation>
                      <xs:documentation>Дата регистрации в качестве индивидуального предпринимателя</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:date"/>
                    </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="РегНом" use="optional">
                    <xs:annotation>
                      <xs:documentation>Регистрационный номер, присвоенный при регистрации</xs:documentation>
                      <xs:documentation>Для индивидуальных предпринимателей, зарегистрированных после 01.01.2004 г., указывается ОГРНИП</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="255"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="НаимРО" use="required">
                    <xs:annotation>
                      <xs:documentation>Наименование органа, зарегистрировавшего ФЛ в качестве ИП</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:minLength value="5"/>
                        <xs:maxLength value="255"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
              <xs:element name="СвКФХ" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Сведения о крестьянском (фермерском) хозяйстве, созданном до 1 января 1995 года, сведения о котором внесены в ЕГРИП при приведении статуса КФХ в соответствие с действующим законодательством</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:attribute name="ДатаНачДейств" use="required">
                    <xs:annotation>
                      <xs:documentation>Дата внесения сведений в ЕГРИП</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:date"/>
                    </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="ГРНИП" type="tns:ОГРНИПТип" use="optional">
                    <xs:annotation>
                      <xs:documentation>Государственный регистрационный номер записи, на основании которой сведения внесены в ЕГРИП</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ОГРН" type="tns:ОГРНТип" use="required">
                    <xs:annotation>
                      <xs:documentation>Основной государственный регистрационный номер крестьянского (фермерского) хозяйства, ранее зарегистрированного в качестве юридического лица</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ИНН" type="tns:ИННЮЛТип" use="optional">
                    <xs:annotation>
                      <xs:documentation>ИНН юридического лица</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="НаимЮЛПолн" use="required">
                    <xs:annotation>
                      <xs:documentation>Полное наименование юридического лица</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="1000"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
              <xs:element name="СвНовЮЛ" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Сведения о производственном кооперативе или хозяйственном товариществе, созданном на базе имущества КФХ</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:attribute name="ДатаНачДейств" use="required">
                    <xs:annotation>
                      <xs:documentation>Дата внесения сведений в ЕГРИП</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:date"/>
                    </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="ГРНИП" type="tns:ОГРНИПТип" use="optional">
                    <xs:annotation>
                      <xs:documentation>Государственный регистрационный номер записи, на основании которой сведения внесены в ЕГРИП</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ОГРН" type="tns:ОГРНТип" use="required">
                    <xs:annotation>
                      <xs:documentation>Основной государственный регистрационный номер юридического лица, созданного на базе имущества крестьянского (фермерского) хозяйства</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ИНН" type="tns:ИННЮЛТип" use="optional">
                    <xs:annotation>
                      <xs:documentation>ИНН юридического лица</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="НаимЮЛПолн" use="required">
                    <xs:annotation>
                      <xs:documentation>Полное наименование юридического лица</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="1000"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
              <xs:element name="СвУчетНО" minOccurs="0">
                <xs:annotation>
